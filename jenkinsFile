pipeline {
    agent any
    
    stages {

        //
        stage("mvnw package") {
            steps {
                sh "sudo -s"
                sh "mvn -N io.takari:maven:wrapper"
                sh "sudo ./mvnw package"
            }
        }
        
        //
        
        stage("java -jar target/*.jar") {
            steps {
                sh "java -jar target/*.jar"
            }
        }
    }
}
